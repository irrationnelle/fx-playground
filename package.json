{
  "name": "fx-exercise",
  "version": "0.0.1",
  "description": "A project to learn fxjs and fxdom",
  "scripts": {
    "build": "webpack",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "rootDir": "./src",
    "transform": {
      ".*": "../node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@fxdom)/",
      "node_modules/(?!@fxjs)/"
    ]
  },
  "keywords": [
    "fxjs",
    "fxdom"
  ],
  "author": "irrationnelle <drakkarverenis@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "fxdom": "^0.8.3",
    "fxjs": "^0.21.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@swc/core": "^1.2.104",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
